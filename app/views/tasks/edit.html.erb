<div class="container">
<h1>Edit your task here</h1>
    <%= form_for(@task) do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title%>
        <%= f.label :details %>
        <%= f.text_field :details %>
        <%= f.label :completed %>
        <%= f.text_field :completed %>
        <%= f.submit %>
    <% end %>
</div>